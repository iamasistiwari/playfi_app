{
  "expo": {
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "splash": {
      "image": "./app.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "web": {
      "output": "static"
    },
    "ios": {
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      },
      "bundleIdentifier": "com.iamasistiwari.app"
    },
    "android": {
      "package": "com.iamasistiwari.app",
      "adaptiveIcon": {
        "foregroundImage": "./app.jpg",
        "backgroundColor": "#121212"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26,
            "compileSdkVersion": 35,
            "targetSdkVersion": 35
          }
        }
      ],
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ],
      [
        "expo-audio",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone.",
          "backgroundAudioPermission": "Allow $(PRODUCT_NAME) to play audio in the background.",
          "audioFocusPermission": "Allow $(PRODUCT_NAME) to control audio focus."
        }
      ]
    ],
    "name": "PlayFi",
    "slug": "app",
    "extra": {
      "router": {
        "origin": "https://n"
      },
      "eas": {
        "projectId": "decfecd2-f376-4f02-be9a-390e06749c80"
      }
    }
  }
}
